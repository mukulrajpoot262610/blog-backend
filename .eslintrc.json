{
    "env": {
        "browser": false,
        "es2021": true
    },
    "ignorePatterns":["dist"],
    "extends": ["airbnb-base", "eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "camelcase": "warn",
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off",
        "import/prefer-default-export": "off",
        "import/no-cycle": "off"
    }
}
